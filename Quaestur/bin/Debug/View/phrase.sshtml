@Master['View/master.sshtml']

@Section['script']
<script>
  function updatePhraseList(){
      $.get("/phrase/list", function(data, status){
          if (data){
              $("#phraseList").html(data);
          }
      });
  }
  $(document).ready(function(){
      updatePhraseList();
  });
</script>
@EndSection

@Section['data']
<div class="container" id="phraseList">
</div>
@EndSection