<div id="editDialog">
  <script>
    $.confirm({
      title: "@Model.PhraseConfirmationTitle",
      content: "@Model.PhraseConfirmationQuestion",
      buttons: {
        confirm: function () {
          $.post("/twofactor/disable/@Model.Id", function(data, status){
            showSecurity();
            $('.modal-backdrop').remove();
          });
        },
        cancel: function () {},
      }
    });
  </script>
</div>