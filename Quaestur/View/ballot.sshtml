@Master['View/master.sshtml']

@Section['script']
<script>
  function updateBallotList(){
      $.get("/ballot/list", function(data, status){
          if (data){
              $("#ballotList").html(data);
          }
      });
  }
  $(document).ready(function(){
      updateBallotList();
  });
</script>
@EndSection

@Section['data']
<div class="container" id="ballotList">
</div>
@EndSection