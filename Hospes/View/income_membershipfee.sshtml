@Each.List
    <div class="form-row row-lightred-background">
      <div class="col-md-4 mb-3">
        <label for="@Current.Id">@Current.Label</label>
        <input type="text" class="form-control" id="@Current.Id" value="@Current.Value" disabled />
      </div>
      <div class="col-md-8 mb-3 align-self-center">
        <span class="align-middle">@Current.Info</span>
      </div>
    </div>
@EndEach